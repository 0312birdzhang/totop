{
    "name": "冲顶大会小助手",
    "description": "冲顶小助手，提前10秒知道题目，祝你冲顶成功~",
    "version": "0.0.2",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions" : ["tabs"],
    "content_scripts":[{
        "matches":["http://*/*", "http://*/", "https://*/*", "https://*/"],
        "js":["jquery.js"]
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "icons": {
      "16": "16.png",
      "48": "48.png",
      "128": "128.png"
    },
    "browser_action": {
        "default_icon": {
            "16": "16.png",
            "48": "48.png",
            "128": "128.png"
        },
        "default_popup": "popup.html"
    }
}
